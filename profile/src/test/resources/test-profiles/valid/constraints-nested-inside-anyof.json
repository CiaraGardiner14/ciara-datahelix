{
  "schemaVersion": "0.1",
  "description": "profile description",
  "rules": [
    {
      "rule": "rule 1",
      "constraints": [
        {
          "anyOf": [
            {
              "field": "field1",
              "is": "formattedAs",
              "value": "%ggg"
            },
            {
              "field": "field1",
              "is": "shorterThan",
              "value": 19
            }
          ]
        }
      ]
    }
  ],
  "fields": [
    {
      "name": "field1"
    }
  ]
}